@import "bootstrap";

.user-info {
  margin-top: 9px;
}

.nav {
  margin-top: 5px;
}

body {
  background-image: url( <%= asset_path("clouds.gif") %> ),url(<%= asset_path("mountains.gif") %>),url(data:image/png;base64,);
  background-repeat: no-repeat,no-repeat,repeat-x;
  background-position: center top, center bottom, 0 0;
  background-color: #ede4ce;
  color: #000;
  text-align: left;
  width: 100%;
}
html,body {
  min-height: 100%;
}

.vote-arrows {
  padding-right: 10px;
  padding-top: 10px;
  text-align: center;
}

.favorite {
  margin-top: 20px;
}

.voted {
  opacity: 0.5;
}

.jumbotron {
  background-image: url(<%= asset_path("trees.jpg") %>);
}
